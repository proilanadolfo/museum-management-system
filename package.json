{
  "name": "museum-management-system",
  "version": "1.0.0",
  "description": "A modern web application for managing museum operations",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run start",
    "client": "cd frontend && npm run dev",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "museum",
    "management",
    "react",
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "Museum Development Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "jspdf-autotable": "^5.0.2",
    "mongoose": "^9.0.0"
  }
}
